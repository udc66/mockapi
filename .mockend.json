{
    "Employee": {
        "id": {
            "int": {
                "min": 1,
                "max": 1000000
            }
        },
        "tabNumber": {
            "regexp": "[0-9]{5}"
        },
        "principalName": {
            "regexp": "[a-z]{5,10}@[a-z]{5}\\.[a-z]{2,3}"
        },
        "firstName": {
            "regexp": "[A-Z][a-z]{5,10}"
        },
        "middleName": {
            "regexp": "[A-Z][a-z]{5,10}"
        },
        "lastName": {
            "regexp": "[A-Z][a-z]{5,10}"
        },
        "gender": {
            "string": [
                "M",
                "F"
            ]
        },
        "dateOfBirth": {
            "dateTime": {
                "min": "1950-01-01T00:00:00Z",
                "max": "2000-12-31T23:59:59Z"
            }
        },        
        "reportTo": {
            "regexp": "[a-z]{5,10}"
        },
        "division": {
            "string": [
                "UDC",
                "UPC",
                "GGT",
                "AAA",
                "HGT"
            ]
        },
        "position": {
            "string": [
                "Developer",
                "Manager",
                "Analyst"
            ]
        },
        "level": {
            "string": [
                "Junior",
                "Middle",
                "Senior"
            ]
        },
        "levelId": {
            "regexp": "[0-9]{2}"
        },
        "roleId": {
            "int": {
                "min": 1,
                "max": 100
            }
        },
        "role": {
            "string": [
                "Developer",
                "Manager",
                "Analyst"
            ]
        },        
        "country": {
            "string": [
                "Ukraine",
                "Austria",
                "Germany",
                "Poland"
            ]
        },
        "contacts":{
            "hasMany": "Contact"
          },
        "projects": {
            "hasMany": "Project"
        }
    },
    "Contact": {
      "body": { "string": {} }
    },   
    "Project": {
      "body": { "string": {} }
    }
}
